@model IEnumerable<ContactsWebApp.Models.Contacts>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Brand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sphere)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cylinder)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Axis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BaseCurve)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MF)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Brand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sphere)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cylinder)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Axis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BaseCurve)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MF)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
